{
  "name": "foobar2000-remote",
  "version": "1.0.0",
  "description": "Control foobar2000 from Node.js using 'foo_httpcontrol' and 'ajquery'.",
  "main": "dist/index",
  "typings": "dist/index",
  "scripts": {
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "lint": "tslint ./src/**.ts",
    "prebuild": "npm run lint && rimraf ./dist",
    "build": "tsc",
    "postbuild": "jsdoc2md ./dist/index.js > DOCUMENTATION.md",
    "watch:build": "tsc --watch"
  },
  "keywords": [
    "foobar2000",
    "media",
    "music"
  ],
  "author": "Olivia Trewin <zancoproductions@gmail.com> (https://github.com/OrionNebula)",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^10.3.2",
    "request": "^2.87.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/mocha": "^5.2.1",
    "@types/request": "^2.47.0",
    "chai": "^4.1.2",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OrionNebula/foobar2000-control.git"
  },
  "bugs": {
    "url": "https://github.com/OrionNebula/foobar2000-control/issues"
  },
  "homepage": "https://github.com/OrionNebula/foobar2000-control#readme"
}
